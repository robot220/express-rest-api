<!DOCTYPE html>
<html ng-app="app">
<head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body ng-controller="MyCtrl">

{{ testValue }}
<div ng-include="url"></div>
<button ng-click="switchTemplate(1)"> To one </button><br/>
<button ng-click="switchTemplate(2)"> To two </button><br/>
<button ng-click="sendReq1()"> Send req #1</button><br/>
<button ng-click="sendReq2()"> Send req #2</button><br/>

<!-- Libraries -->
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular-route.min.js"></script>
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.25/angular-resource.min.js"></script>


<script>
    var app = angular.module('app', []);
    app.controller('MyCtrl', MyCtrl);

    function MyCtrl($scope, $http){

        $scope.testValue = 100;
        $scope.switchTemplate = function(id){
            $scope.url = "../views/" + id + ".html";
        }
        $scope.url = "../views/1.html";
        $scope.sendReq1 = function(){
            $http({
                method: 'post',
                url: 'http://192.168.0.103:3001/authenticate',
                data: { "name": "Bob", "password": "pass1234", "token": "" }
            }).then(function(response){
                console.log(response);
            });
        };

        $scope.sendReq2 = function(){
            $http({
                method: 'get',
                url: 'http://192.168.0.104:3001/random',
                //headers: { "Authorization": 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..osIjtLwLHZ-jmYUQeqY7L8t-WnlcKaAZHwzp0ahe1cM' }
                //data: { "token": 'eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..osIjtLwLHZ-jmYUQeqY7L8t-WnlcKaAZHwzp0ahe1cM' }
            }).then(function(response){
                console.log(response);
            });
        };

    }

    MyCtrl.$inject = [
        "$scope",
        "$http"
    ];

</script>
</body>
</html>